{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "projects": {
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "jwt": {
      "type": "library",
      "root": "libs/jwt",
      "entryFile": "index",
      "sourceRoot": "libs/jwt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/jwt/tsconfig.lib.json"
      }
    },
    "regex": {
      "type": "library",
      "root": "libs/regex",
      "entryFile": "index",
      "sourceRoot": "libs/regex/src",
      "compilerOptions": {
        "tsConfigPath": "libs/regex/tsconfig.lib.json"
      }
    },
    "password-encoder": {
      "type": "library",
      "root": "libs/password-encoder",
      "entryFile": "index",
      "sourceRoot": "libs/password-encoder/src",
      "compilerOptions": {
        "tsConfigPath": "libs/password-encoder/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "aws": {
      "type": "library",
      "root": "libs/aws",
      "entryFile": "index",
      "sourceRoot": "libs/aws/src",
      "compilerOptions": {
        "tsConfigPath": "libs/aws/tsconfig.lib.json"
      }
    },
    "upload-file": {
      "type": "library",
      "root": "libs/upload-file",
      "entryFile": "index",
      "sourceRoot": "libs/upload-file/src",
      "compilerOptions": {
        "tsConfigPath": "libs/upload-file/tsconfig.lib.json"
      }
    },
    "sms": {
      "type": "library",
      "root": "libs/sms",
      "entryFile": "index",
      "sourceRoot": "libs/sms/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sms/tsconfig.lib.json"
      }
    },
    "twillo": {
      "type": "library",
      "root": "libs/twillo",
      "entryFile": "index",
      "sourceRoot": "libs/twillo/src",
      "compilerOptions": {
        "tsConfigPath": "libs/twillo/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json",
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {}
      }
    ]
  },
  "monorepo": true,
  "root": "apps/api"
}